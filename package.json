{
  "name": "@kaasino/pulse-sdk",
  "version": "1.0.0",
  "description": "Lightweight performance monitoring SDK for Kaasino",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "performance",
    "monitoring",
    "web-vitals",
    "analytics",
    "kaasino"
  ],
  "author": "Kaasino",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=18"
  }
}
